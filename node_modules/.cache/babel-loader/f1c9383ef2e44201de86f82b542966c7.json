;AACA,MAAA,YAAA,CAAA,OAAA,CAAA,SAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,OAAA,CAAA;AACA,KA5BA;AA6BA,IAAA,SA7BA,qBA6BA,IA7BA,EA6BA;AACA,UAAA,SAAA,GAAA,KAAA,OAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,WAAA,SAAA,GAAA,KAAA,OAAA,CAAA,SAAA,EAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,SAAA;AACA,KAjCA;AAkCA,IAAA,YAlCA,wBAkCA,IAlCA,EAkCA;AACA,UAAA,CAAA,MAAA,CAAA,IAAA,IAAA,CAAA,MAAA,CAAA,UAAA,IAAA,CAAA,MAAA,CAAA,QAAA,IAAA,CAAA,MAAA,CAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,wDAAA,CAAA;AACA;AACA;;AAEA,UAAA,KAAA,GAAA,QAAA,CAAA,cAAA,CAAA,WAAA,CAAA;;AACA,UAAA,CAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,0CAAA,CAAA;AACA,OAFA,MAGA,IAAA,CAAA,KAAA,CAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,2EAAA,CAAA;AACA,OAFA,MAGA,IAAA,CAAA,KAAA,CAAA,KAAA,CAAA,CAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,6CAAA,CAAA;AACA,OAFA,MAGA;AACA,YAAA,IAAA,GAAA,IAAA,CAAA,KAAA;AACA,YAAA,EAAA,GAAA,IAAA,UAAA,EAAA;AACA,QAAA,EAAA,CAAA,aAAA,CAAA,IAAA;AAEA,YAAA,IAAA,GAAA,IAAA,IAAA,CAAA,CAAA,IAAA,CAAA,EAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA,CAAA,CALA,CAKA;;AAEA,YAAA,IAAA,GAAA,QAAA,CAAA,aAAA,CAAA,GAAA,CAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,MAAA,CAAA,GAAA,CAAA,eAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,CAAA,QAAA,GAAA,IAAA,CAAA,IAAA;AACA,QAAA,IAAA,CAAA,KAAA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AAEA,KAhEA;AAiEA,IAAA,QAjEA,oBAiEA,IAjEA,EAiEA;AACA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,UAAA,OAAA,GAAA,IAAA,CAAA,OAAA;;AACA,UAAA,KAAA,GAAA,IAAA,EAAA,IAAA,KAAA,IAAA,IAAA,EAAA,EAAA;AACA,QAAA,IAAA,CAAA,OAAA,GAAA,KAAA,MAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,IAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,IAAA;AACA,OAJA,MAIA,IAAA,KAAA,IAAA,IAAA,EAAA,IAAA,KAAA,MAAA,IAAA,EAAA,EAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,KAAA,GAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,IAAA;AACA,QAAA,IAAA,CAAA,OAAA,GAAA,OAAA;AACA,OAJA,MAIA,IAAA,KAAA,GAAA,IAAA,EAAA,IAAA,KAAA,MAAA,IAAA,EAAA,EAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,KAAA,IAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,IAAA;AACA,QAAA,IAAA,CAAA,OAAA,GAAA,OAAA;AACA;;AACA,MAAA,YAAA,CAAA,OAAA,GAAA,IAAA,CAAA,SAAA,CAAA,KAAA,OAAA,CAAA;AACA;AArFA,GAZA;AAmGA,EAAA,OAnGA,qBAmGA;AACA,SAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,CADA,CAEA;AACA;AAtGA,CAAA","sourcesContent":["<template>\n    <div class=\"main-wrapper\">\n        <div v-for=\"data in details\" v-bind:key=\"data.id\" class=\"card-wrapper\">\n            <img :src=\"getImgUrl(data.path)\" class=\"img-wrapper\" />\n            <h1 class=\"name\"><mark>Name:</mark> {{data.name}}</h1>\n            <p><mark>Rating:</mark> 10/{{data.ratings}}</p>\n            <b-button v-b-modal.edit-modal @click=\"elementId(data)\" type=\"button\" class=\"btn btn-warning btn-adjust\">Edit</b-button>\n            <b-button @click=\"deleteList(data)\" class=\"btn btn-danger btn-adjust mt-2\">Delete</b-button>\n            <b-button @click=\"downloadItem(data)\"  id=\"downloadImg\" variant=\"outline-success\" class=\"mt-2\">Download</b-button>\n\n        \n            <b-modal id=\"edit-modal\" title=\"Edit Details\" v-if=\"data.id == clickedId\" hide-footer centered>\n                <b-form>\n                                <img v-if=\"toggleImg1 && data.id == clickedId\" :src=\"imgUrl()\" class=\"img-wrapper\" />\n                                <img v-if=\"toggleImg2 && data.id == clickedId\" :src=\"getImgUrl(data.path)\" class=\"img-wrapper\" />\n                        <b-form-file accept=\"image/jpeg, image/png, image/gif\" @change=\"getFile\" class=\"input-file\"></b-form-file>\n                        <b-form-input v-model=\"text\"  v-bind:placeholder=\"data.name\"></b-form-input>\n                        <b-form-rating id=\"rating-10\" variant=\"warning\" v-model=\"rating\" stars=\"10\"></b-form-rating>\n                        <b-button type=\"submit\" @click=\"onSubmit(data)\" variant=\"primary\" class=\"btn btn-success upload-btn\">Upload</b-button>\n            </b-form>\n            </b-modal>\n    </div>\n    </div> \n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            details: [],\n            clickedId: null,\n            rating: '',\n            img: '',\n            toggleImg1: false,\n            toggleImg2: true,\n            text: '',\n        }\n    },\n    methods: {\n        getImgUrl(pic) {\n        return require('../assets/'+pic)\n    },\n    // downloadItem (data) {\n    // get(data.path, { responseType: 'blob' })\n    //   .then(response => {\n    //     const blob = new Blob([response.data], { type: 'application/pdf' })\n    //     const link = document.createElement('a')\n    //     link.href = URL.createObjectURL(blob)\n    //     link.download = label\n    //     link.click()\n    //     URL.revokeObjectURL(link.href)\n    //   }).catch(console.error)\n//   },\n    imgUrl() {\n        return require('../assets/'+this.img)\n    },\n    getFile(event) {\n            this.img = event.target.files[0].name;\n            this.toggleImg1 = true;\n            this.toggleImg2 = false;\n            console.log(event)\n        },\n    deleteList(data) {\n            const dataIndex = this.details.indexOf(data);\n            this.details.splice(dataIndex, 1)\n            localStorage.setItem('details', JSON.stringify(this.details));\n        },\n    elementId(data) {\n        const dataIndex = this.details.indexOf(data);\n        this.clickedId = this.details[dataIndex].id;\n        console.log(this.clickedId)\n    },\n    downloadItem(data) {\n        if (!window.File || !window.FileReader || !window.FileList || !window.Blob) {\n                    alert('The File APIs are not fully supported in this browser.');\n                    return;\n                }\n\n                var input = document.getElementById('fileinput');\n                if (!input) {\n                    alert(\"Um, couldn't find the fileinput element.\");\n                }\n                else if (!input.files) {\n                    alert(\"This browser doesn't seem to support the `files` property of file inputs.\");\n                }\n                else if (!input.files[0]) {\n                    alert(\"Please select a file before clicking 'Load'\");\n                }\n                else {\n                    var file = data.files;\n                    var fr = new FileReader();\n                    fr.readAsDataURL(file);\n\n                    var blob = new Blob([file], { type: \"image/png\" });// change resultByte to bytes\n\n                    var link = document.createElement('a');\n                    link.href = window.URL.createObjectURL(blob);\n                    link.download = data.path;\n                    link.click();\n                }\n                console.log(data)\n      \n    },\n    onSubmit(data) {\n        // const dataIndex = this.details.indexOf(data);\n        console.log(data.id);\n        let name = data.name;\n        let path = data.path;\n        let ratings = data.ratings;\n        if(this.img == '' && this.text == '') {\n            data.ratings = this.rating;\n            data.name = name;\n            data.path = path;\n        }else if(this.text == '' && this.rating == '') {\n             data.path =  this.img;\n             data.name = name;\n             data.ratings = ratings;\n        }else if(this.img == '' && this.rating == '') {\n            data.name =  this.text;\n            data.path = path;\n            data.ratings = ratings\n        }        \n        localStorage.details = JSON.stringify(this.details)\n    }\n    },\n    mounted() {\n        this.details = JSON.parse(localStorage.details)\n        // console.log(this.details)\n    }\n    }\n</script>\n\n<style scoped>\n.main-wrapper {\n        display: grid;\n        grid-template-columns: repeat(auto-fill, 200px);\n        gap: 30px;\n        position: relative;\n        justify-content: center;\n        margin-bottom: 60px;\n        z-index: 1;\n        top: 51px;\n        padding: 43px;\n        margin-left: 49px;\n        margin-right: 49px;\n        background-color: #33a2b8;\n    }\n    .card-wrapper {\n            text-align: center;\n            -webkit-box-shadow: 0px 0px 13px -2px rgba(0, 0, 0, 0.29);\n            -moz-box-shadow: 0px 0px 13px -2px rgba(0, 0, 0, 0.29);\n            box-shadow: 0px 0px 13px -2px rgba(0, 0, 0, 0.29);\n            padding-bottom: 8px;\n            display: grid;\n            animation-name: power;\n            width: 200px;\n            animation-duration: 1s;\n            background-color: seashell;\n        }\n        .img-wrapper {\n                height: 300px;\n                width: 100%;\n                object-fit: cover;\n            }\n        .name {\n            word-break: break-all;\n            font-size: 15px;\n            width: 100%;\n            font-weight: bold;\n            margin-top: 10px;\n        }\n        .btn-adjust {\n            width: 78px;\n            justify-self: center;\n        }\n        mark {\n            background-color: yellow;\n            font-weight: bold;\n        }\n@keyframes power {\n    0% {\n        transform: scale(0.5, 0.5);\n    }\n    100% {\n        transform: scale();\n    }\n}\n</style>"],"sourceRoot":"src/components"}]}