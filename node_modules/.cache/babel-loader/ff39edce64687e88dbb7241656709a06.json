AA,CAAA;AACA,KAjBA;AAkBA,IAAA,SAlBA,qBAkBA,IAlBA,EAkBA;AACA,UAAA,SAAA,GAAA,KAAA,OAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,WAAA,SAAA,GAAA,KAAA,OAAA,CAAA,SAAA,EAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,SAAA;AACA,KAtBA;AAuBA,IAAA,QAvBA,oBAuBA,IAvBA,EAuBA;AACA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,EAAA;;AACA,UAAA,KAAA,IAAA,KAAA,EAAA,IAAA,KAAA,GAAA,KAAA,EAAA,EAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,KAAA,IAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,KAAA,GAAA;AACA;;AACA,MAAA,IAAA,CAAA,MAAA,GAAA,KAAA,MAAA;AAEA,MAAA,YAAA,CAAA,OAAA,GAAA,IAAA,CAAA,SAAA,CAAA,KAAA,OAAA,CAAA;AACA;AAjCA,GAZA;AA+CA,EAAA,OA/CA,qBA+CA;AACA,SAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,CADA,CAEA;AACA;AAlDA,CAAA","sourcesContent":["<template>\n    <div class=\"main-wrapper\">\n        <div v-for=\"data in details\" v-bind:key=\"data.id\" class=\"card-wrapper\">\n            <img :src=\"getImgUrl(data.path)\" class=\"img-wrapper\" />\n            <h1 class=\"name\"><mark>Name:</mark> {{data.name}}</h1>\n            <p><mark>Rating:</mark> 10/{{data.ratings}}</p>\n            <button v-b-modal.edit-modal @click=\"elementId(data)\" type=\"button\" class=\"btn btn-warning btn-delete\">Edit</button>\n            <button @click=\"deleteList(data)\" class=\"btn btn-danger btn-delete\">Delete</button>\n        \n            <b-modal id=\"edit-modal\" title=\"Edit Details\" v-if=\"data.id == clickedId\" hide-footer centered>\n                <b-form>\n                                <img v-if=\"toggleImg1 && data.id == clickedId\" v-bind:src=\"imgUrl()\" class=\"img-wrapper\" />\n                                <img v-if=\"toggleImg2 && data.id == clickedId\" v-bind:src=\"getImgUrl(data.path)\" class=\"img-wrapper\" />\n                        <b-form-file accept=\"image/jpeg, image/png, image/gif\" @change=\"getFile\" class=\"input-file\"></b-form-file>\n                        <b-form-input v-model=\"text\"  v-bind:placeholder=\"data.name\"></b-form-input>\n                        <b-form-rating id=\"rating-10\" variant=\"warning\" v-model=\"rating\" stars=\"10\"></b-form-rating>\n                        <b-button type=\"submit\" @click=\"onSubmit(data)\" variant=\"primary\" class=\"btn btn-success upload-btn\">Upload</b-button>\n            </b-form>\n            </b-modal>\n    </div>\n    </div> \n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            details: [],\n            clickedId: null,\n            rating: null,\n            img: '',\n            toggleImg1: false,\n            toggleImg2: true,\n            text: '',\n        }\n    },\n    methods: {\n        getImgUrl(pic) {\n        return require('../assets/'+pic)\n    },\n    imgUrl() {\n        return require('../assets/'+this.img)\n    },\n    getFile(event) {\n            this.img = event.target.files[0].name;\n            this.toggleImg1 = true;\n            this.toggleImg2 = false;\n            console.log(this.img)\n        },\n    deleteList(data) {\n            const dataIndex = this.details.indexOf(data);\n            this.details.splice(dataIndex, 1)\n            localStorage.setItem('details', JSON.stringify(this.details));\n        },\n    elementId(data) {\n        const dataIndex = this.details.indexOf(data);\n        this.clickedId = this.details[dataIndex].id;\n        console.log(this.clickedId)\n    },\n    onSubmit(data) {\n        // const dataIndex = this.details.indexOf(data);\n        console.log(data.id);\n        if(this.text !== '' && this.img !== '') {\n              data.name =  this.text;\n              data.path =  this.img;\n        }\n                data.rating = this.rating\n        \n        localStorage.details = JSON.stringify(this.details)\n    }\n    },\n    mounted() {\n        this.details = JSON.parse(localStorage.details)\n        // console.log(this.details)\n    }\n    }\n</script>\n\n<style scoped>\n.main-wrapper {\n        display: grid;\n        grid-template-columns: repeat(auto-fill, 200px);\n        gap: 30px;\n        position: relative;\n        justify-content: center;\n        margin-bottom: 60px;\n        z-index: 1;\n        top: 51px;\n        padding: 43px;\n        margin-left: 49px;\n        margin-right: 49px;\n        background-color: beige;\n    }\n    .card-wrapper {\n            text-align: center;\n            -webkit-box-shadow: 0px 0px 13px -2px rgba(0, 0, 0, 0.29);\n            -moz-box-shadow: 0px 0px 13px -2px rgba(0, 0, 0, 0.29);\n            box-shadow: 0px 0px 13px -2px rgba(0, 0, 0, 0.29);\n            padding-bottom: 8px;\n            display: grid;\n            animation-name: power;\n            width: 200px;\n            animation-duration: 1s;\n            background-color: seashell;\n        }\n        .img-wrapper {\n                height: 300px;\n                width: 100%;\n                object-fit: cover;\n            }\n        .name {\n            word-break: break-all;\n            font-size: 15px;\n            width: 100%;\n            font-weight: bold;\n            margin-top: 10px;\n        }\n        .btn-delete {\n            width: 78px;\n            justify-self: center;\n        }\n        mark {\n            background-color: yellow;\n            font-weight: bold;\n        }\n@keyframes power {\n    0% {\n        transform: scale(0.5, 0.5);\n    }\n    100% {\n        transform: scale();\n    }\n}\n</style>"],"sourceRoot":"src/components"}]}