   fileDatabase: []\n        }\n    },\n    methods: {\n        onFileSelected(event) {\n            if(this.selectedFile == []) {\n                this.selectedFile = event.target.files[0].name\n            }\n            else {\n                this.selectedFile.push({\n                    \"name\": event.target.files[0].name,\n                })\n                // console.log(this.selectedFile)\n            }\n            // console.log(event)\n        },\n        onUpload() {\n            this.selectedFile.map((d,i) => {\n                if(this.fileDatabase.name !== d.name) {\n                    this.fileDatabase.push({\n                        \"name\": d.name,\n                        \"id\": i\n                    })\n                }\n            })\n            // this.fileDatabase = this.selectedFile\n            console.log(this.fileDatabase)\n            localStorage.savedData = JSON.stringify(this.fileDatabase)\n        }\n    },\n    // mounted() {\n    //     saveData(fileDatabase) {\n    //         this.\n    //     }\n    // }\n}\n</script>\n\n<style scoped>\n    .main-container {\n        display: grid;\n        justify-content: center;\n        gap: 103px;\n        margin-top: 40px;\n    }\n    .input-file {\n        padding: 120px;\n        background-color: #33a2b8;\n        box-shadow: 5px 10px #888888;\n        border-radius: 155px;\n        text-align-last: center;\n    }\n    .upload-btn {\n        max-width: 100px;\n        justify-self: center;\n    }\n</style>"]}]}